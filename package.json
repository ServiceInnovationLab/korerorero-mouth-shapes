{
  "name": "korerorero-mouth-shapes",
  "version": "0.0.1",
  "description": "Exposing DanielSWolf / rhubarb-lip-sync as a service",
  "main": "index.js",
  "scripts": {
    "install": "run-s vendor:*",
    "vendor:dir-check-clone": "[ -d 'vendor/rhubarb-lip-sync' ]  && echo 'ℹ️ Directory vendor/rhubarb-lip-sync exists.' || run-s clone",
    "vendor:disable-extras": "sed -i '/^add_subdirectory(\"extras/d' vendor/rhubarb-lip-sync/CMakeLists.txt",
    "vendor:compile": "cmake-js compile -Wno-dev --CDBoost_NO_BOOST_CMAKE=ON -d vendor/rhubarb-lip-sync  ",
    "clone": "mkdir vendor; cd vendor; git clone https://github.com/DanielSWolf/rhubarb-lip-sync",
    "clean": "rm -rf vendor/rhubarb-lip-sync",
    "start": "node dist/index.js",
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "cmake-js": "^6.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "jest": "^25.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ServiceInnovationLab/korerorero-mouth-shapes.git"
  },
  "author": "Conrad Johnston",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ServiceInnovationLab/korerorero-mouth-shapes/issues"
  },
  "homepage": "https://github.com/ServiceInnovationLab/korerorero-mouth-shapes#readme"
}
